{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "packages/*"
      ]
    },
    "forceConsistentCasingInFileNames": true,
  }
}
